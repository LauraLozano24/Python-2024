      "source": [
        "#  codigo fibonacci\n",
        "\n",
        "def fibonacci(n):\n",
        "  if n < 0:\n",
        "    raise ValueError(\"el valor de n  no puede ser negativo\")\n",
        "  if n in [0, 1]:\n",
        "    return n\n",
        "  else:\n",
        "    return fibonacci(n - 1) + fibonacci(n - 2)\n",
        "\n",
        "for i in range(10):\n",
        "  print(fibonacci(i))\n"
      ],
